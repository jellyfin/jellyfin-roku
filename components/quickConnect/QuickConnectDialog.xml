        <Timer id="quickConnectTimer" duration="3" repeat="false" />
    </children>
    <interface>
        <field id="quickConnectJson" type="assocarray" />
        <field id="authenticated" type="boolean" alwaysNotify="true" />
        <field id="saveCredentials" type="boolean" />
    </interface>
</component>