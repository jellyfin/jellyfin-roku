<?xml version="1.0" encoding="utf-8"?>
<component name="TVEpisodeList" extends="MarkupGrid">
  <children>
    <Rectangle
      id="scrollBar"
      width="6"
      height="390"
      translation="[1650, 0]">
      <Rectangle
        id="thumb"
        height="100"
        width="6" />
    </Rectangle>
    <Animation id="scrollBarThumbSlideAnimation" duration=".4" repeat="false">
      <Vector2DFieldInterpolator id="scrollBarThumbSlideInterpolator" key="[0.0, 1.0]" keyValue="[[0, 0], [0, 0]]" fieldToInterp="thumb.translation" />
    </Animation>
  </children>
  <interface>
    <field id="objects" type="assocarray" onChange="setupRows" />
    <field id="escapeButton" type="string" alwaysNotify="true" />
    <field id="doneLoading" type="boolean" />
  </interface>
</component>