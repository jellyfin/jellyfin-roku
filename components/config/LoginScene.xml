<?xml version="1.0" encoding="utf-8"?>
<component name="LoginScene" extends="JFGroup">
  <children>
    <label text="Enter Configuration"
      id="prompt"
      font="font:LargeBoldSystemFont"
      translation="[150, 150]" />
    <ConfigList
      id="configOptions"
      translation="[150, 240]" />
    <CheckList
      id="onOff"
      translation="[200, 450]" />
    <Button
      id="submit"
      text="Submit"
      translation="[150, 550]" />
    <Button
      id="quickConnect"
      translation="[550, 550]"
    />
    <label text=""
      id="alert"
      wrap="true"
      width="1620"
      font="font:MediumSystemFont"
      translation="[150, 680]" />
    <Timer
      id="quickConnectTimer"
      duration="3"
      repeat="true" />
  </children>
</component>