{
    "files": [
        "manifest",
        "source/**/*.*",
        "components/**/*.*",
        "images/**/*.*",
        "resources/**/*.*",
        "locale/**/*.*",
        "settings/*.*"
    ],
    "plugins": [
        "@rokucommunity/bslint",
        "rooibos-roku"
    ],
    "diagnosticFilters": [
        "node_modules/**",
        "**/roku_modules/**"
    ]
}